#!/bin/bash

#      RHS Power Card

python LpdPowerControl.py --hvbias 50 --femI2cBus 512
python LpdPowerControl.py --lv 1 --hv 1 --femI2cBus 512

#      LHS Power Card

python LpdPowerControl.py --hvbias 50 --femI2cBus 768
python LpdPowerControl.py --lv 1 --hv 1 --femI2cBus 768

echo "-=-=-=-=-=-=-=-=-=-=-=-=-         POWER CARDS' INFO:         -=-=-=-=-=-=-=-=-=-=-=-=-"

python LpdSuperModulePowerStatus.py